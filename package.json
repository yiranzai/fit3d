{
  "name": "fit3d-diverse-maps",
  "version": "1.0.0",
  "description": "Fit3D多样化开源地图样式系统 - 支持多种开源地图提供商和样式的户外运动数据可视化",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run build:web",
    "build:web": "vite build",
    "dev": "concurrently \"npm run dev:core\" \"npm run dev:web\"",
    "dev:core": "tsc --watch",
    "dev:web": "vite",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "format": "prettier --write src tests",
    "start": "node dist/index.js",
    "cli": "node dist/cli/index.js",
    "serve": "node dist/web/server.js",
    "db:migrate": "node dist/scripts/migrate.js",
    "db:seed": "node dist/scripts/seed.js",
    "cache:cleanup": "node dist/scripts/cache-cleanup.js",
    "comment:scan": "node dist/scripts/comment-scanner.js",
    "comment:translate": "node dist/scripts/comment-translator.js"
  },
  "keywords": [
    "fit3d",
    "maps",
    "openstreetmap",
    "cartodb",
    "stamen",
    "esri",
    "outdoor",
    "sports",
    "visualization",
    "chinese",
    "开源地图",
    "户外运动",
    "数据可视化"
  ],
  "author": "Fit3D Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "sqlite3": "^5.1.6",
    "duckdb": "^1.9.2",
    "leaflet": "^1.9.4",
    "mapbox-gl": "^2.15.0",
    "ol": "^8.2.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "sharp": "^0.32.6",
    "turf": "^6.5.0",
    "redis": "^4.6.10",
    "ws": "^8.14.2",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "canvas": "^2.11.2",
    "proj4": "^2.9.0",
    "geojson": "^0.5.0",
    "mbtiles": "^0.12.0",
    "tar": "^6.2.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/ws": "^8.5.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/cheerio": "^0.22.35",
    "@types/proj4": "^2.5.5",
    "@types/geojson": "^7946.0.14",
    "@types/tar": "^6.1.9",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "playwright": "^1.40.0",
    "@playwright/test": "^1.40.0",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0",
    "concurrently": "^8.2.2",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/fit3d.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/fit3d/issues"
  },
  "homepage": "https://fit3d.dev"
}
